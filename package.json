{
  "name": "dpr-js",
  "version": "1.0.0",
  "description": "Steps:",
  "main": "index.js",
  "scripts": {
    "build": "babel-node tools/build.js",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "node_modules/.bin/jest",
    "test:travis": "npm run test && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "start": "npm-run-all --parallel open:src lint:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anuveyatsu/dpr-js.git"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less)$": "<rootDir>/mocks/styleMock.js"
    },
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "collectCoverage": true,
    "verbose": true,
    "testRegex": "(/tests/.*).jsx?$",
    "globals": {
      "DATA_PACKAGE_URL": ""
    },
    "unmockedModulePathPatterns": [
      "react",
      "plotly.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/anuveyatsu/dpr-js/issues"
  },
  "homepage": "https://github.com/anuveyatsu/dpr-js#readme",
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-cli": "6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-plugin-transform-runtime": "6.4.3",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.17.0",
    "browser-sync": "2.18.5",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.15",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "dotenv": "4.0.0",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.5.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.8.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "isparta": "4.0.0",
    "istanbul": "0.4.5",
    "jest": "^18.0.0",
    "json-loader": "^0.5.4",
    "moment-timezone": "0.5.11",
    "nock": "^9.0.2",
    "npm-run-all": "3.1.2",
    "postcss-loader": "1.2.1",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-logger": "^2.4.0",
    "redux-mock-store": "^1.2.1",
    "replace": "0.3.0",
    "sinon": "^1.17.7",
    "style-loader": "0.13.1",
    "webpack": "^1.14.0",
    "webpack-bundle-analyzer": "2.1.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "datapackage": "^0.8.1",
    "datapackage-render": "git+https://github.com/frictionlessdata/datapackage-render-js.git",
    "handsontable": "^0.29.2",
    "isomorphic-fetch": "2.2.1",
    "leaflet": "1.0.3 ",
    "lodash": "^4.0.0",
    "plotly.js": "^1.21.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "react-spinkit": "^2.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "vega": "^2.6.5",
    "vega-embed": "^2.2.0",
    "vega-lite": "^1.3.1"
  }
}
